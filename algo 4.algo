PROCEDURE insertion__sort(var arr:array of integer)
VAR
    i,j,current :integer;
BEGIN
    FOR i FROM 1 TO arr.length-1   DO
        current := arr[i];
        j:= i-1;
        WHILE (j>=0 and arr[j]>current) DO
            arr[j+1]:=arr[j];
            j:=j-1
            END_WHILE
            arr[j+1]=current
        
    END_FOR
END